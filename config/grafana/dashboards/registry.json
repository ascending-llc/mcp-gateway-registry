  "templating": {
    "list": [
      {
        "name": "server_name",
        "label": "Server Name",
        "type": "query",
        "query": "label_values(mcp_tool_execution_total, server_name)",
        "refresh": 1,
        "includeAll": true,
        "allValue": ".*"

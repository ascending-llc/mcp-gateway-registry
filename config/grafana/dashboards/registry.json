{
  "id": null,
  "uid": "registry-metrics",
  "title": "Registry - Tool Execution & Operations",
  "description": "Registry tool execution metrics, operation rates, and latency analysis - optimized for demos",
  "tags": [
    "registry",
    "tools",
    "execution",
    "otel",
    "demo"
  ],
  "timezone": "browser",
  "refresh": "10s",
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "annotations": {
    "list": []
  },
  "panels": [
    {
      "id": 100,
      "title": "Tool Execution Overview",
      "type": "row",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "collapsed": false
    },
    {
      "id": 1,
      "title": "Tool Execution Rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(mcp_tool_execution_total{status=\"success\"}[5m]))",
          "legendFormat": "Success"
        },
        {
          "expr": "sum(rate(mcp_tool_execution_total{status=\"failure\"}[5m]))",
          "legendFormat": "Failure"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 1
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": [
            "mean",
            "max",
            "last"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "axisLabel": "Executions/sec",
            "axisPlacement": "left",
            "fillOpacity": 10,
            "lineWidth": 2
          },
          "unit": "reqps"
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Success"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "green",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Failure"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "id": 2,
      "title": "Tool Execution Success Rate (%)",
      "type": "gauge",
      "targets": [
        {
          "expr": "sum(rate(mcp_tool_execution_total{status=\"success\"}[5m])) / sum(rate(mcp_tool_execution_total[5m])) * 100",
          "legendFormat": "Success Rate"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 12,
        "y": 1
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "orange",
                "value": 90
              },
              {
                "color": "yellow",
                "value": 95
              },
              {
                "color": "green",
                "value": 99
              }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "title": "Tool Execution Latency (P50/P95/P99)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(mcp_tool_execution_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P50"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(mcp_tool_execution_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P95"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(mcp_tool_execution_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P99"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 18,
        "y": 1
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": [
            "mean",
            "max",
            "last"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "axisLabel": "Latency",
            "axisPlacement": "left",
            "fillOpacity": 10,
            "lineWidth": 2
          },
          "unit": "s"
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "P50"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "green",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "P95"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "yellow",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "P99"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "id": 101,
      "title": "Key Metrics Summary",
      "type": "row",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 9
      },
      "collapsed": false
    },
    {
      "id": 4,
      "title": "Execution Statistics",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(increase(mcp_tool_execution_total[1h]))",
          "legendFormat": "Executions (1h)"
        },
        {
          "expr": "sum(rate(mcp_tool_execution_total{status=\"success\"}[5m])) / sum(rate(mcp_tool_execution_total[5m])) * 100",
          "legendFormat": "Success Rate"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(mcp_tool_execution_duration_seconds_bucket[5m])) by (le)) * 1000",
          "legendFormat": "P95 Latency (ms)"
        },
        {
          "expr": "count(count by (tool_name)(mcp_tool_execution_total))",
          "legendFormat": "Unique Tools"
        }
      ],
      "gridPos": {
        "h": 5,
        "w": 24,
        "x": 0,
        "y": 10
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "textMode": "auto",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "blue",
                "value": null
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Success Rate"
            },
            "properties": [
              {
                "id": "unit",
                "value": "percent"
              },
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    {
                      "color": "red",
                      "value": null
                    },
                    {
                      "color": "yellow",
                      "value": 95
                    },
                    {
                      "color": "green",
                      "value": 99
                    }
                  ]
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "P95 Latency (ms)"
            },
            "properties": [
              {
                "id": "unit",
                "value": "ms"
              },
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    {
                      "color": "green",
                      "value": null
                    },
                    {
                      "color": "yellow",
                      "value": 500
                    },
                    {
                      "color": "red",
                      "value": 2000
                    }
                  ]
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Unique Tools"
            },
            "properties": [
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    {
                      "color": "purple",
                      "value": null
                    }
                  ]
                }
              }
            ]
          }
        ]
      }
    },
    {
      "id": 102,
      "title": "Tool Execution Details",
      "type": "row",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 15
      },
      "collapsed": false

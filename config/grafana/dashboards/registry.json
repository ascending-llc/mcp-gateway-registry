    {
      "id": 5,
      "title": "Tool Execution by Server",
      "type": "table",
      "targets": [
        {
          "expr": "sum(increase(mcp_tool_execution_total{status=\"success\"}[1h])) by (server_name)",
          "legendFormat": "{{server_name}}",
          "format": "table",
          "instant": true,
          "refId": "A"
        },
        {
          "expr": "sum(increase(mcp_tool_execution_total{status=\"failure\"}[1h])) by (server_name)",
          "legendFormat": "{{server_name}}_failures",
          "format": "table",
          "instant": true,
          "refId": "B"
        },
        {
          "expr": "sum by (server_name) (increase(mcp_tool_execution_total{status=\"success\"}[1h])) / (sum by (server_name) (increase(mcp_tool_execution_total{status=\"success\"}[1h])) + sum by (server_name) (increase(mcp_tool_execution_total{status=\"failure\"}[1h])) + 0.0001) * 100",
          "legendFormat": "{{server_name}}_success_rate",
          "format": "table",
          "instant": true,
          "refId": "C"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      },
      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true
            },
            "renameByName": {
              "server_name": "Server",
              "Value #A": "Successes (1h)",
              "Value #B": "Failures (1h)",
              "Value #C": "Success Rate %"
            }
          }
        }
      ],
      "fieldConfig": {
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Success Rate %"
            },
            "properties": [
              {
                "id": "unit",
                "value": "percent"
              },
              {
                "id": "decimals",
                "value": 2
              },
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    {
                      "color": "red",
                      "value": null
                    },
                    {
                      "color": "yellow",
                      "value": 90
                    },
                    {
                      "color": "green",
                      "value": 99
                    }
                  ]
                }
              },
              {
                "id": "custom.cellOptions",
                "value": {
                  "type": "color-background"
                }
              }
            ]
          }
        ]
      }
    },

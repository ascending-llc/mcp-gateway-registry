{
  "id": null,
  "uid": "mcp-otel-metrics",
  "title": "MCP Gateway - OTel Metrics Dashboard",
  "description": "OpenTelemetry metrics for Auth, Tool Discovery, Tool Execution, and Registry Operations",
  "tags": [
    "mcp",
    "otel",
    "auth",
    "tools",
    "registry",
    "latency"
  ],
  "timezone": "browser",
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "annotations": {
    "list": []
  },
  "panels": [
    {
      "id": 100,
      "title": "Auth Request Rate & Success Rate",
      "type": "row",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "collapsed": false
    },
    {
      "id": 1,
      "title": "Auth Request Rate (per second)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(auth_requests_total{status=\"success\"}[5m]))",
          "legendFormat": "Success"
        },
        {
          "expr": "sum(rate(auth_requests_total{status=\"failure\"}[5m]))",
          "legendFormat": "Failure"
        },
        {
          "expr": "sum(rate(auth_requests_total[5m]))",
          "legendFormat": "Total"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 1
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": [
            "mean",
            "max",
            "last"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "axisLabel": "Requests/sec",
            "axisPlacement": "left",
            "fillOpacity": 10,
            "lineWidth": 2
          },
          "unit": "reqps"
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Success"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "green",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Failure"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Total"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "blue",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "id": 2,
      "title": "Auth Success Rate (%)",
      "type": "gauge",
      "targets": [
        {
          "expr": "sum(rate(auth_requests_total{status=\"success\"}[5m])) / sum(rate(auth_requests_total[5m])) * 100",
          "legendFormat": "Success Rate"

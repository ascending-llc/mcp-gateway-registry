{
  "id": null,
  "uid": "mcpgw-metrics",
  "title": "MCP Gateway - Protocol & Discovery",
  "description": "MCP Gateway protocol activity, tool discovery, and server request metrics",
  "tags": [
    "mcp",
    "gateway",
    "discovery",
    "protocol",
    "otel"
  ],
  "timezone": "browser",
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "annotations": {
    "list": []
  },
  "panels": [
    {
      "id": 100,
      "title": "MCP Protocol Overview",
      "type": "row",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "collapsed": false
    },
    {
      "id": 1,
      "title": "Real-time Protocol Activity",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(mcp_tool_executions_total{method=\"initialize\", success=\"True\"}[1m]))",
          "legendFormat": "Initialize Rate"
        },
        {
          "expr": "sum(rate(mcp_tool_executions_total{method=\"tools/list\", success=\"True\"}[1m]))",
          "legendFormat": "Tools List Rate"
        },
        {
          "expr": "sum(rate(mcp_tool_executions_total{method=\"tools/call\", success=\"True\"}[1m]))",
          "legendFormat": "Tool Call Rate"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 1
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": [
            "mean",
            "max",
            "last"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "axisLabel": "Requests per Second",
            "axisPlacement": "left",
            "fillOpacity": 10,
            "lineWidth": 2
          },
          "unit": "reqps"
        }
      }
    },
    {
      "id": 2,
      "title": "MCP Server Requests",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(mcp_server_requests_total[5m])) by (server_name)",
          "legendFormat": "{{server_name}}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 1
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": [
            "mean",
            "max",
            "last"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "axisLabel": "Requests/sec",
            "axisPlacement": "left",
            "fillOpacity": 10,
            "lineWidth": 2
          },
          "unit": "reqps"
        }
      }
    },
    {
      "id": 101,
      "title": "Gateway Statistics",
      "type": "row",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 9
      },
      "collapsed": false
    },
    {
      "id": 3,
      "title": "Gateway Key Metrics",
      "type": "stat",
      "targets": [
        {
          "expr": "count(count by (server_name)(mcp_tool_executions_total))",
          "legendFormat": "Active Servers"
        },
        {
          "expr": "sum(increase(mcp_tool_executions_total[1h]))",
          "legendFormat": "Total Requests (1h)"
        },
        {
          "expr": "sum(increase(mcp_tool_discovery_total{status=\"success\"}[1h]))",
          "legendFormat": "Discoveries (1h)"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(mcp_tool_discovery_duration_seconds_bucket[5m])) by (le)) * 1000",
          "legendFormat": "Discovery P95 (ms)"
        }
      ],
      "gridPos": {
        "h": 5,
        "w": 24,
        "x": 0,
        "y": 10
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "textMode": "auto",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "blue",
                "value": null
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Active Servers"
            },
            "properties": [
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    {
                      "color": "green",
                      "value": null
                    }
                  ]
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Discovery P95 (ms)"
            },
            "properties": [
              {
                "id": "unit",
                "value": "ms"
              },
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    {
                      "color": "green",
                      "value": null
                    },
                    {
                      "color": "yellow",
                      "value": 500
                    },
                    {
                      "color": "red",
                      "value": 1000
                    }
                  ]
                }
              }
            ]
          }
        ]
      }
    },
    {
      "id": 102,
      "title": "Tool Discovery",
      "type": "row",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 15
      },
      "collapsed": false
    },
    {
      "id": 4,
      "title": "Tool Discovery Rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(mcp_tool_discovery_total{status=\"success\"}[5m]))",
          "legendFormat": "Success"
        },
        {
          "expr": "sum(rate(mcp_tool_discovery_total{status=\"failure\"}[5m]))",
          "legendFormat": "Failure"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 16
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": [
            "mean",
            "max",
            "last"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "axisLabel": "Discoveries/sec",
            "axisPlacement": "left",
            "fillOpacity": 10
          },
          "unit": "reqps"
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Success"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "green",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Failure"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "id": 5,
      "title": "Tool Discovery Latency (P50/P95/P99)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(mcp_tool_discovery_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P50"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(mcp_tool_discovery_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P95"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(mcp_tool_discovery_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P99"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 8,
        "y": 16
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": [
            "mean",
            "max",
            "last"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "axisLabel": "Latency",
            "axisPlacement": "left",
            "fillOpacity": 10,
            "lineWidth": 2
          },
          "unit": "s"
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "P50"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "green",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "P95"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "yellow",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "P99"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "id": 6,
      "title": "Tool Discovery Latency Stats",
      "type": "stat",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(mcp_tool_discovery_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P50"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(mcp_tool_discovery_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P95"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(mcp_tool_discovery_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P99"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 16,
        "y": 16
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "textMode": "auto",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 0.5
              },
              {
                "color": "red",
                "value": 1
              }
            ]
          }
        }
      }
    },
    {
      "id": 7,
      "title": "Tool Discovery by Source",
      "type": "piechart",
      "targets": [
        {
          "expr": "sum(increase(mcp_tool_discovery_total[1h])) by (source)",
          "legendFormat": "{{source}}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 24
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": [
            "value",
            "percent"
          ]
        },
        "pieType": "donut"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "short"
        }
      }
    },
    {
      "id": 8,
      "title": "Tool Discovery by Transport Type",
      "type": "piechart",
      "targets": [
        {
          "expr": "sum(increase(mcp_tool_discovery_total[1h])) by (transport_type)",
          "legendFormat": "{{transport_type}}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 8,
        "y": 24
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": [
            "value",
            "percent"
          ]
        },
        "pieType": "donut"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "short"
        }
      }
    },
    {
      "id": 9,
      "title": "Tools Discovered per Server",
      "type": "bargauge",
      "targets": [
        {
          "expr": "sum(mcp_tool_discovery_tools_count) by (server_name)",
          "legendFormat": "{{server_name}}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 16,
        "y": 24
      },
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "continuous-BlPu"
          },
          "unit": "short"
        }
      }
    },
    {
      "id": 103,
      "title": "Protocol Methods & Clients",
      "type": "row",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 32
      },
      "collapsed": false
    },
    {
      "id": 10,
      "title": "MCP Protocol Methods Distribution",
      "type": "bargauge",
      "targets": [
        {
          "expr": "topk(10, sum(mcp_tool_executions_total{method!=\"tools/call\"}) by (method))",
          "legendFormat": "{{method}}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 33
      },
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "short"
        }
      }
    },
    {
      "id": 11,
      "title": "Client Applications Distribution",
      "type": "bargauge",
      "targets": [
        {
          "expr": "topk(10, sum(mcp_tool_executions_total) by (client_name))",
          "legendFormat": "{{client_name}}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 8,
        "y": 33
      },
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "short"
        }
      }
    },
    {
      "id": 12,
      "title": "Session Activity by Client",
      "type": "bargauge",
      "targets": [
        {
          "expr": "topk(15, sum(rate(mcp_tool_executions_total[5m])) by (client_name))",
          "legendFormat": "{{client_name}}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 16,
        "y": 33
      },
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "continuous-GrYlRd"
          },
          "unit": "reqps"
        }
      }
    },
    {
      "id": 104,
      "title": "Server Performance",
      "type": "row",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 41
      },
      "collapsed": false
    },
    {
      "id": 13,
      "title": "Server Performance Dashboard",
      "type": "table",
      "targets": [
        {
          "expr": "sum(mcp_tool_executions_total) by (server_name)",
          "legendFormat": "{{server_name}}_total_calls",
          "format": "table",
          "instant": true,
          "refId": "A"
        },
        {
          "expr": "sum(increase(mcp_tool_executions_total[1h])) by (server_name)",
          "legendFormat": "{{server_name}}_calls_per_hour",
          "format": "table",
          "instant": true,
          "refId": "B"
        },
        {
          "expr": "count(count by (server_name, tool_name)(mcp_tool_executions_total)) by (server_name)",
          "legendFormat": "{{server_name}}_unique_tools",
          "format": "table",
          "instant": true,
          "refId": "C"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 42
      },
      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true
            },
            "renameByName": {
              "server_name": "Server",
              "Value #A": "Total Calls",
              "Value #B": "Calls/Hour",
              "Value #C": "Unique Tools"
            }
          }
        }
      ]
    },
    {
      "id": 14,
      "title": "MCP Protocol Flow Analysis",
      "type": "table",
      "targets": [
        {
          "expr": "sum(mcp_tool_executions_total{method=\"initialize\"}) by (client_name)",
          "legendFormat": "{{client_name}}_init",
          "format": "table",
          "instant": true,
          "refId": "A"
        },
        {
          "expr": "sum(mcp_tool_executions_total{method=\"tools/list\"}) by (client_name)",
          "legendFormat": "{{client_name}}_list",
          "format": "table",
          "instant": true,
          "refId": "B"
        },
        {
          "expr": "sum(mcp_tool_executions_total{method=\"tools/call\"}) by (client_name)",
          "legendFormat": "{{client_name}}_call",
          "format": "table",
          "instant": true,
          "refId": "C"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 42
      },
      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true
            },
            "renameByName": {
              "client_name": "Client",
              "Value #A": "Initialize",
              "Value #B": "Tools List",
              "Value #C": "Tool Calls"
            }
          }
        }
      ]
    },
    {
      "id": 105,
      "title": "Error Analysis",
      "type": "row",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 50
      },
      "collapsed": false
    },
    {
      "id": 15,
      "title": "Discovery Error Rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(mcp_tool_discovery_total{status=\"failure\"}[5m])) / sum(rate(mcp_tool_discovery_total[5m])) * 100",
          "legendFormat": "Discovery Error Rate %"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 51
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": [
            "mean",
            "max",
            "last"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "axisLabel": "Error Rate %",
            "axisPlacement": "left",
            "fillOpacity": 10,
            "lineWidth": 2
          },
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 1
              },
              {
                "color": "red",
                "value": 5
              }
            ]
          }
        }
      }
    },
    {
      "id": 16,
      "title": "Protocol Error Rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(mcp_tool_executions_total{success=\"False\"}[5m])) / sum(rate(mcp_tool_executions_total[5m])) * 100",
          "legendFormat": "Protocol Error Rate %"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 51
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": [
            "mean",
            "max",
            "last"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "axisLabel": "Error Rate %",
            "axisPlacement": "left",
            "fillOpacity": 10,
            "lineWidth": 2
          },
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 1
              },
              {
                "color": "red",
                "value": 5
              }
            ]
          }
        }
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "server_name",
        "label": "Server Name",
        "type": "query",
        "query": "label_values(mcp_tool_executions_total, server_name)",
        "refresh": 1,
        "includeAll": true,
        "allValue": ".*"
      },
      {
        "name": "client_name",
        "label": "Client Name",
        "type": "query",
        "query": "label_values(mcp_tool_executions_total, client_name)",
        "refresh": 1,
        "includeAll": true,
        "allValue": ".*"
      },
      {
        "name": "method",
        "label": "Method",
        "type": "query",
        "query": "label_values(mcp_tool_executions_total, method)",
        "refresh": 1,
        "includeAll": true,
        "allValue": ".*"
      },
      {
        "name": "transport_type",
        "label": "Transport Type",
        "type": "query",
        "query": "label_values(mcp_tool_discovery_total, transport_type)",
        "refresh": 1,
        "includeAll": true,
        "allValue": ".*"
      }
    ]
  },
  "schemaVersion": 39,
  "version": 1
}
